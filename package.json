{
  "name": "vlc-rpc",
  "version": "4.0.2",
  "description": "Discord Rich Presence integration for VLC Media Player for Windows - Show your friends what you're watching or listening to",
  "main": "./out/main/index.js",
  "author": {
    "name": "Valentin Marquez",
    "email": "valentin13.mail@gmail.com",
    "url": "https://github.com/valentin-marquez"
  },
  "homepage": "https://github.com/valentin-marquez/vlc-rpc",
  "repository": {
    "type": "git",
    "url": "https://github.com/valentin-marquez/vlc-rpc.git"
  },
  "license": "MIT",
  "keywords": [
    "vlc",
    "discord",
    "rich-presence",
    "media-player",
    "discord-rpc",
    "electron",
    "windows"
  ],
  "scripts": {
    "format": "biome format --write .",
    "lint": "biome check --write --unsafe .",
    "lint:fix": "biome check --write .",
    "lint:files": "biome check --write",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:force-update": "cross-env FORCE_UPDATE_CHECK=1 electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "electron-vite build && electron-builder --win",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish"
  },
  "dependencies": {
    "@changesets/cli": "^2.29.6",
    "@ctrl/video-filename-parser": "^5.4.0",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@nanostores/persistent": "^0.10.2",
    "@nanostores/react": "^0.8.4",
    "@radix-ui/react-icons": "^1.3.2",
    "@tailwindcss/vite": "^4.0.15",
    "@xhayper/discord-rpc": "^1.3.0",
    "cheerio": "^1.1.2",
    "clsx": "^2.1.1",
    "electron-conf": "^1.3.0",
    "electron-updater": "^6.3.9",
    "electron-winston": "^1.0.0",
    "nanostores": "^0.11.4",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.15",
    "wouter": "^3.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.29.7",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/cheerio": "^1.0.0",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^10.0.0",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  },
  "packageManager": "bun@1.1.13"
}
